{
    "name": "boostworld/comment-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides threaded comment functionality for Symfony applications - Fork for PHP8",
    "keywords": ["comment", "threads", "forum"],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Nagel",
            "email": "tim@nagel.com.au"
        },
        {
            "name": "Alexander Mols",
            "email": "iam.asm89@gmail.com"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/friendsofsymfony/FOSCommentBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.4",
        "friendsofsymfony/rest-bundle": "^3.0",
        "jms/serializer-bundle": "^2.0|^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.2",
        "symfony/asset": "^3.4|^4.4 || ^5.4 || ^6.2",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.2",
        "symfony/routing": "^4.4 || ^5.4 || ^6.2",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.2",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.2",
        "symfony/form": "^4.4 || ^5.4 || ^6.2",
        "symfony/validator": "^4.4 || ^5.4 || ^6.2",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.2",
        "symfony/twig-bridge": "^4.4 || ^5.4 || ^6.2",
        "twig/twig": "^2.0|^3.0",
        "phpoption/phpoption": "^1.1",
        "doctrine/annotations": "~1.0|^2"
    },
    "suggest": {
        "friendsofsymfony/user-bundle": "Allows for user integration.",
        "ornicar/akismet-bundle": "Integrate Akismet for spam detection.",
        "symfony/acl-bundle": "Integrates the ACL Security component.",
        "symfony/assetic-bundle": "Integrates Assetic into Symfony."
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4 || ^5.4 || ^6.2",
        "symfony/dom-crawler": "^4.4 || ^5.4 || ^6.2",
        "symfony/css-selector": "^4.4 || ^5.4 || ^6.2",
        "doctrine/orm": "^2.3",
        "doctrine/doctrine-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^3.0",
        "ornicar/akismet-bundle": "dev-master",
        "symfony/expression-language": "^4.4 || ^5.4 || ^6.2",
        "symfony/phpunit-bridge": "^4.4 || ^5.4 || ^6.2",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "FOS\\CommentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\CommentBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/thanks": false
        }
    }
}
